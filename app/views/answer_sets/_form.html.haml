= form_for @answer_set do |f|
  - if @answer_set.errors.any?
    #error_explanation
      %h2= "#{pluralize(@answer_set.errors.count, "error")} prohibited this answer_set from being saved:"
      %ul
        - @answer_set.errors.full_messages.each do |msg|
          %li= msg

  .actions
    = f.submit 'Save'
